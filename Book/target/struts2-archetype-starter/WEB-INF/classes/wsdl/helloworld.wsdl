            </xs:element>
            <xs:element name="outputSOAShoppingCart">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element type="tns:shoppingCart" name="result" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="shoppingCart">
                <xs:sequence>
                    <xs:element name="cartid" type="xs:int"/>
                    <xs:element name="user_id" type="xs:int"/>
                    <xs:element name="duration" type="xs:int"/>
                    <xs:element name="statut" type="xs:boolean"/>
                </xs:sequence>
            </xs:complexType>

        </xs:schema>
