            <xs:element name="outputSOAUserAdd">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element type="xs:string" name="result"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:complexType name="user">
                <xs:sequence>
                    <xs:element name="id" type="xs:int"/>
                    <xs:element name="pseudo" type="xs:string"/>
                    <xs:element name="password" type="xs:string"/>
                    <xs:element name="gender" type="xs:string"/>
                    <xs:element name="mail" type="xs:string"/>
                </xs:sequence>
            </xs:complexType>

            <xs:element name="inputSOARent">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element type="xs:int" name="id"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="outputSOARent">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element type="tns:rent" name="result" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="rent">
                <xs:sequence>
                    <xs:element name="id" type="xs:int"/>
                    <xs:element name="book_id" type="xs:int"/>
                    <xs:element name="user_id" type="xs:int"/>
                    <xs:element name="create_at" type="xs:date"/>
                    <xs:element name="end_at" type="xs:date"/>
                    <xs:element name="reload" type="xs:boolean"/>
                    <xs:element name="returnbook" type="xs:boolean"/>
                </xs:sequence>
            </xs:complexType>

            <xs:element name="inputSOAShoppingBook">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element type="xs:int" name="id"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="outputSOAShoppingBook">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element type="tns:shoppingBook" name="result" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="shoppingBook">
                <xs:sequence>
                    <xs:element name="id" type="xs:int"/>
                    <xs:element name="book_id" type="xs:int"/>
                    <xs:element name="user_id" type="xs:int"/>
                    <xs:element name="create_at" type="xs:date"/>
                    <xs:element name="end_at" type="xs:date"/>
                    <xs:element name="reload" type="xs:boolean"/>
                    <xs:element name="returnbook" type="xs:boolean"/>
